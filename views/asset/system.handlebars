
<hr>
<div class="racks">
    <h3>{{systemName}} </h3> 
    
    <ul id="eachUl"class="list-unstyled"> 
        <li>
        <div class="row">
            
            <div class="col-md-4">

            <strong>System Details</strong> <br>
            
            Status: {{{systemStatusLit}}}<br>
            
            {{#if systemEnviron}}Environment: {{systemEnviron}}<br>{{/if}}
            {{#if systemRole}}Role: {{systemRole}}<br>{{/if}}
            {{#if systemOwner}}Owner: {{systemOwner}}<br>{{/if}}
        {{!--    {{#if systemImpact}}Impact: {{systemImpact}}<br>{{/if}}  --}}
            {{#if systemIsVirtual}}Is Virtual: {{systemIsVirtual}}<br>{{/if}}
            {{#if systemParentId}}Parent: {{systemParentId}}<br>{{/if}}
            Ticket: {{{systemTicketLit}}}<br>
            
            
            </div>
            <div class="col-md-4">

            {{#if systemEquipSN}}
            <strong>Hardware Details</strong><br>
            Equipment SN: <strong><a href="/equipment/{{systemEquipSN}}">{{systemEquipSN}}</a></strong><br>
            Location: <strong><a href="/equipment-systems/{{equipLocationRack}}">{{equipLocation}}</a></strong><br>
            Equipment Status: <strong>{{equipStatus}}</strong><br>
            {{#if equipMake}}{{equipMake}}{{#if equipModel}} {{equipModel}}{{/if}}{{#if equipSubModel}} - {{equipSubModel}}{{/if}}<br>{{/if}}
            
            {{#if equipType}}{{#if equipRUHieght}}{{equipRUHieght}}U {{/if}}{{equipType}}<br>{{/if}}
            {{/if}}
          
            </div>
            <div class="col-md-2">
            </div>
            <div class="col-md-2">
                {{> assets/system-e-c}}
            </div>
        </div>
        <div class="row">
            

            {{> assets/sysport-list}}
         </div>   
         <div class="row">
            
            <div class="col-md-4">           
            
            {{#if systemIsVirual}} Is Virtual <br>{{/if}}

            <strong>OS and Software</strong><br>
            {{#if systemOSType}}OS: {{systemOSType}}<br>{{/if}}
            {{#if systemOSVersion}}OS Version: {{systemOSVersion}}<br>{{/if}}
            {{#if systemApplications}}Aplications: {{systemApplications}}<br>{{/if}}
            {{#if systemSupLic}}Support License: {{systemSupLic}}<br>{{/if}}
            {{#if systemSupEndDate}}Support End Date: {{systemSupEndDate}}<br>{{/if}}
            <br>
            </div>

            <div class="col-md-4">         
            <strong>Timing</strong><br>
            {{#if systemInstall}}Install Date: {{systemInstall}}<br>{{/if}}
            {{#if systemStart}}Start Date: {{systemStart}}<br>{{/if}}
            {{#if systemEnd}}End Date: {{systemEnd}}<br>{{/if}}
            <br>
            {{#if systemNotes}}Notes: {{systemNotes}}<br>{{/if}}
            {{#if createdBy}}Created By: {{createdBy}}{{#if createdOn}} {{createdOn}}{{/if}}<br>{{/if}}
            Modified {{#if modifiedBy}}By: {{modifiedBy}}{{/if}}{{#if modifiedOn}}: {{modifiedOn}}<br>{{/if}}
            </div>
            <div class="col-md-2">
            
            </div>
            <div class="col-md-2">
            
            


            
            </div>
            </div>
        
        </li><br>
    </ul>
    <hr>
    <div class="row">
        {{> assets/config-portconfig}}  
        {{> assets/config-acs6k}}
        {{#if ilom}}          {{> assets/config-ilom}}        {{/if}}
        {{#if isServTechPDU}} {{> assets/config-servtechpdu}} {{/if}}
        {{#if isACS6000}}     {{> assets/config-bareACS6k}}   {{/if}}
    </div>    
</div>

