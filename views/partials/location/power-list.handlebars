<div>
`{{#if access.edit}}
    <form id="powerAddForm" role="form" action="/location/rack/circuit~{{rackUnique}}>new" method="GET"></form>
        <a href="#" onclick="document.getElementById('powerAddForm').submit();return false;">
                    <button type="button" class="btn btn-danger btn-sm">
                    <span class="glyphicon glyphicon-edit"></span> Power</button></a>
        {{else}}<h4>Power</h4>{{/if}}

  <table {{> bootstrap-table}} >
    <thead>
      <tr>
        {{#if access.edit}}<th data-sortable="true" data-field="rackEdit">Edit</th>{{/if}}
        <th data-sortable="true" data-field="rackPowUnique">Circuit</th>
        <th data-sortable="true" data-field="rackPowStatus">Status</th>
        <th data-sortable="true" data-field="rackPowVolts">Volts</th>
        <th data-sortable="true" data-field="rackPowPhase">Phase</th>
        <th data-sortable="true" data-field="rackPowAmps">Amps</th>
        <th data-sortable="true" data-field="rackPowReceptacle ">Receptical</th>
        <th data-sortable="true" data-field="createdOn">Created On</th>
        <th data-sortable="true" data-field="modifiedOn">Last Modified</th>
      </tr>
    </thead>
    <tbody>
    {{#each powers}}
      <tr>
        <td>
          <a href="/location/rack/circuit~{{../rackUnique}}>{{rackPowId}}" >
          <span class="glyphicon glyphicon-edit glyphicon-danger"></span></a>
          <a href="/location/rack/circuitcopy~{{../rackUnique}}>{{rackPowId}}">
          <span class="glyphicon glyphicon-plus-sign glyphicon-danger"></span></a>
        </td>
        <td><a href="/location/rack/circuit~{{../rackUnique}}>{{rackPowId}}"  target="_blank">{{rackPowUnique}}</a></td>
        <td>{{rackPowStatus}}</td>
        <td>{{rackPowVolts}}</td>
        <td>{{rackPowPhase}}</td>
        <td>{{rackPowAmps}}</td>
        <td>{{rackPowReceptacle}}</td>
        <td>{{createdOn}}</td>
        <td>{{modifiedOn}}</td>
      </tr>
    {{/each}}
  </tbody>
  </table>
</div>

