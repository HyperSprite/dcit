<div> 
      <div id="dvData">
    <table id="toCSV" class="table-sort table-sort-search " style="overflow: scroll;">
        <thead>
            <tr>
                <th class="table-sort" data-sort="dcNetEdit">Edit</th>
                <th class="table-sort" data-sort="dcNetType">ipV</th>
                <th class="table-sort" data-sort="dcNetNickname">Network</th>
                <th class="table-sort" data-sort="dcNetVlan">vLan</th>
                <th class="table-sort" data-sort="dcNetDesc">Description</th>
                <th class="table-sort" data-sort="dcNetGateway">Gateway</th>
                <th class="table-sort" data-sort="dcNetDomain">Domain</th>
                <th class="table-sort" data-sort="dcNetSettings">Settings</th>
            </tr>
        </thead>
        <tbody class="list">
            {{#each networks}}
                <tr>
                    <td class="dcNetEdit">
                {{#if ../access.edit}}
                    <form id="netEditForm{{@index}}" role="form" action="/location/network/{{id}}-{{dcNetId}}" method="GET"></form>
                    <form id="netCopyForm{{@index}}" role="form" action="/location/network/{{id}}-{{dcNetId}}~copy" method="GET"></form>          
                <a href="#" onclick="document.getElementById('netEditForm{{@index}}').submit();return false;" title="Edit">
                    <span class="glyphicon glyphicon-edit glyphicon-danger"></span></a>
                
                <a href="#" onclick="document.getElementById('netCopyForm{{@index}}').submit();return false;" title="Copy">
                    <span class="glyphicon glyphicon-plus-sign glyphicon-danger"></span></a>
                    {{/if}}  {{@index}}</td>
                    <td class="dcNetType">{{dcNetType}}</td>     
                    <td class="dcNetNickname">{{dcNetUnique}}<br><br>Start {{ipLowStr}}<br>End {{ipHighStr}}<br>Mask {{prefixMaskStr}}</td>
                    <td class="dcNetVlan">{{dcNetVlan}}</td>
                    <td class="dcNetDesc">{{{dcNetDesc}}}</td>
                    <td class="dcNetGateway">{{dcNetGateway}}</td>
                    <td class="dcNetDomain">{{dcNetDomain}}</td>
                    <td class="dcNetSettings">
                        {{#if iPRangeipLowStr}}Low Range: {{iPRangeipLowStr}}<br>
                                    High Range: {{iPRangeipHighStr}}<br>
                                    Mask String: {{iPRangeprefixMaskStr}}<br>{{/if}}
                        {{#if dcNetDns1}}Primary DNS: {{dcNetDns1}}<br>{{/if}}
                        {{#if dcNetDns2}}Secondary DNS: {{dcNetDns2}}<br>{{/if}}
                        {{#if dcNetNTP1}}Primary NTP: {{dcNetNTP1}}<br>{{/if}}
                        {{#if dcNetNTP2}}Secondary NTP: {{dcNetNTP2}}<br>{{/if}}
                        {{#if dcNetLdap1}}Primary LDAP: {{dcNetLdap1}}<br>{{/if}}
                        {{#if dcNetLdap2}}Secondary LDAP:{{dcNetLdap2}}<br>{{/if}}
                        {{#if dcNetLdapString}}Secondary LDAP:{{dcNetLdapString}}<br>{{/if}}
                        {{#if dcNetTftpHost}}TFTP Host: {{dcNetTftpHost}}<br>{{/if}}
                        {{#if dcNetACSFilePath}}ACS TFTP File Path: {{dcNetACSFilePath}}<br>{{/if}}
                        
                    </td>
                </tr>  
            {{/each}}
        </tbody>
    </table>
    </div>
         {{> tocsvpartial }}
    </div>

