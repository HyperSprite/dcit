<ol class="breadcrumb">
  <li><a href="/">Home</a></li>
  <li><a href="/location/datacenter/list">Datacenters</a></li>
  <li><a href="/location/datacenter/{{abbreviation}}">{{abbreviation}}</a></li>
  <li>Edit Contacts<li>
</ol>
<h2>{{fullName}} - {{abbreviation}}</h2>
{{#if foundingCompany}}{{foundingCompany}}<br>{{/if}}


<div class="datacenter"> 

<div class="formContainer">
    <form id="datacenterForm"class="form-horizontal datacenterForm" role="form" action="/location/datacentercontact/{{abbreviation}}" method="POST">
		<input type="hidden" name="_csrf" value="{{_csrfToken}}">
        <input type="hidden" name="id" value="{{id}}">
        <input type="hidden" name="abbreviation" value={{abbreviation}}>
   
    <ul id="eachUl"class="list-unstyled">    
        {{#each contacts}}
        <li id="eachLi-{{@index}}" class="eachLi">
        <hr>
        <input type="hidden" name="index" value="{{@index}}">
        <input type="hidden" name="conId" value="{{id}}">
        <div class="form-group">
            <label for="fieldconType" class="col-sm-2 control-label"  >Type</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="fieldconType{{@index}}" name="conType"  value="{{conType}}" placeholder="Contact Type?">
            </div>
        </div>
         <div class="form-group">
            <label for="fieldconName" class="col-sm-2 control-label"  >Name</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="fieldconName{{@index}}" name="conName"  value="{{conName}}" placeholder="Contact Name?">
            </div>
        </div>      
         <div class="form-group">
            <label for="fieldconEmail" class="col-sm-2 control-label"  >Email</label>
            <div class="col-sm-4">
                <input type="email" class="form-control" id="fieldconEmail{{@index}}" name="conEmail" value="{{conEmail}}" placeholder="Email Address?">
            </div>
        </div>        
         <div class="form-group">
            <label for="fieldContact.conURL" class="col-sm-2 control-label" >URL</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="fieldconURL{{@index}}" name="conURL" value="{{conURL}}" placeholder="URL">
            </div>
        </div>

        
        <div class="form-group">
            <label for="fieldAddress1" class="col-sm-2 control-label">Address</label>
            <div class="col-sm-4">
				<input type="text" class="form-control" id="fieldAddress1{{@index}}" name="address1" value="{{address1}}" placeholder="To ... Care of ? ">
