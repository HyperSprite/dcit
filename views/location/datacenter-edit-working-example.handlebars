

<h2>{{fullName}} - {{abbreviation}}</h2>


<div class="datacenter">

    {{#each contacts}}
    {{#if conName}}Name: {{conName}}<br>{{/if}}
    {{#if conType}}Type: {{conType}}<br>{{/if}}
    {{#if address1}}Address: {{{address}}}<br>{{/if}}
    {{#if city}}{{city}}, {{state}} {{zip}}<br>{{/if}}
    {{#if lat}}<a href="https://www.google.com/maps/place/{{lat}},{{lon}}">Find on Google</a><br>{{/if}}
    {{#if conEmail}}Email: <a href="mailto:{{conEmail}}" target="_top">{{conEmail}}</a><br>{{/if}}
    {{#if conURL}}URL: <a href="{{conURL}}" target="_top">{{conURL}}</a><br>{{/if}}
            {{#each conPhones}}
            Phone: {{conPhoNumber}} Type: {{conPhoType}}<br>
            {{/each}}
    <br>
    {{/each}}
    Created On: {{createdOn}}
</div>

<div class="formContainer">
    <form class="form-horizontal datacenterForm" role="form" action="/location/datacenter" method="POST">
		<input type="hidden" name="_csrf" value="{{_csrfToken}}">
        <input type="hidden" name="id" value="{{id}}">
        <div class="form-group">
            <label for="fieldFullName" class="col-sm-2 control-label">Full Name</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="fieldFullName" name="fullName" value="{{fullName}}" placeholder="Name">
            </div>
        </div>
        <div class="form-group">
            <label for="fieldAbbreviation" class="col-sm-2 control-label"  >Abbreviation</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="fieldAbbreviation" name="abbreviation"  value="{{abbreviation}}"placeholder="Abbreviation">
            </div>
        </div>
        <div class="form-group">
            <label for="fieldFoundingCompany" class="col-sm-2 control-label"  >Founding Company</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="fieldFoundingCompany" name="foundingCompany"  value="{{foundingCompany}}"placeholder="Founding Company">
            </div>
        </div>       
        
        
